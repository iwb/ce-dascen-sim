{
  "experiment_id": "exp05_scheduled_delivery_deterministic",
  "description": "Deterministic scheduled deliveries with high quality products (baseline for exp06 comparison)",
  "output_prefix": "scheduled_baseline",
  
  "simulation": {
    "weeks": 2,
    "start_date": "2025-01-01T00:00:00"
  },
  
  "factory_structure": {
    "file": "workshop_parallel_2.json",
    "distance_matrix": "distance_matrix_workshop_parallel_2.csv"
  },
  
  "products": [
    {"file": "variants/variant1.json", "enabled": true},
    {"file": "variants/variant2.json", "enabled": true},
    {"file": "variants/variant3.json", "enabled": true}
  ],
  
  "product_delivery": {
    "mode": "scheduled",
    "schedule_file": "schedule_deterministic.json"
  },
  
  "overrides": {
    "behavior_mode": "deterministic",
    "output": {
      "core_outputs": {
        "export_eventlog": true,
        "export_case_table": true,
        "export_output_table": true
      }
    }
  }
}